  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/AndreasPizsa/grunt-update-json.git"
  },
  "bugs": {
    "url": "https://github.com/AndreasPizsa/grunt-update-json/issues"
  },
  "license": "MIT",
  "main": "Gruntfile.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
